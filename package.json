{
  "name": "@chatie/graphql",
  "version": "0.3.1",
  "description": "Chatie GraphQL Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "gc:deploy:prod": "graphcool deploy -t prod",
    "gc:deploy:dry": "graphcool deploy --dry-run",
    "gc:local:deploy": "graphcool deploy -c local -n chatie -t dev",
    "gc:local:info": "graphcool info -t dev",
    "gc:local:bootstrap": "./scripts/local-bootstrap.sh",
    "gc:local:pull": "graphcool local pull",
    "gc:local:destroy": "./scripts/local-destroy.sh",
    "gc:local:reset": "echo 'not work' || echo y | graphcool reset -t dev",
    "gc:local:stop": "graphcool local stop",
    "gc:local:up": "graphcool local up",
    "gc:login": "graphcool login",
    "dist": "npm run clean && tsc",
    "lint": "npm run lint:ts",
    "lint:ts": "echo tslint v`tslint --version` && tslint --project tsconfig.json --exclude \"tests/fixtures/**\" --exclude \"dist/\" && tsc --noEmit",
    "gc:playground": "graphcool playground",
    "generate-schema": "npm run gc:local:up && ts-node scripts/generate-schema",
    "test:pack": "npm run gc:local:bootstrap && npm run dist && export TMPDIR=/tmp/pack-testing.$$ && npm pack && mkdir $TMPDIR && mv *-*.*.*.tgz $TMPDIR && cp tests/fixtures/pack-testing.js $TMPDIR && cd $TMPDIR && npm init -y && npm i *-*.*.*.tgz && (for i in {1..3}; do node pack-testing.js && break || sleep 1; done)",
    "test:unit": "blue-tape -r ts-node/register -r source-map-support/register \"tests/**/*.spec.ts\" \"src/**/*.spec.ts\"",
    "test": "npm run clean && npm run gc:local:destroy && npm run gc:local:bootstrap && npm run lint && npm run test:unit"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.6",
    "apollo-link-batch-http": "^1.2.1",
    "apollo-link-ws": "^1.0.7",
    "brolog": "^1.2.8",
    "graphcool": "^0.11.5",
    "graphcool-lib": "^0.1.4",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwks-rsa": "^1.2.1",
    "node-fetch": "^2.1.1",
    "read-pkg-up": "^3.0.0",
    "subscriptions-transport-ws": "^0.9.6"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^9.4.6",
    "@types/read-pkg-up": "^3.0.1",
    "@types/ws": "^4.0.1",
    "apollo-codegen": "^0.19.0",
    "babel-eslint": "^8.2.2",
    "blue-tape": "^1.0.0",
    "cuid": "^2.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.17.0",
    "git-scripts": "^0.2.1",
    "graphql-tag": "^2.8.0",
    "shx": "^0.2.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "git": {
    "scripts": {
      "pre-push": "./scripts/pre-push.sh"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatie/graphql.git"
  },
  "author": {
    "name": "Huan LI",
    "email": "zixia@zixia.net",
    "url": "https://www.zixia.net"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/chatie/graphql/issues"
  },
  "homepage": "https://github.com/chatie/graphql#readme",
  "files": [
    "dist/*",
    "src/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "bin": {
    "chatie-graphql-local-bootstrap": "scripts/local-bootstrap.sh",
    "chatie-graphql-local-destroy": "scripts/local-destroy.sh"
  }
}
